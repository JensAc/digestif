package = {
  name = "tikz.sty",
  dependencies = {
    "tikz"
  }
}
environments = {
  tikzpicture = {
    arguments = {
      {
        delimiters = {"[", "]"},
        keys = "$ref:tikz#/keys/tikz",
        meta = "options",
        optional = true
      },
    },
    documentation = "texdoc:generic/pgf/pgfmanual.pdf#pgf.tikzpicture"
  },
  scope = {
    arguments = {
      {
        delimiters = {"[", "]"},
        keys = "$ref:tikz#/keys/tikz",
        meta = "options",
        optional = true
      },
    },
    documentation = "texdoc:generic/pgf/pgfmanual.pdf#pgf.scope"
  },
}

